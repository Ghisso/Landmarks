<?xml version="1.0" encoding="UTF-8"?>
<Shell xmlns="http://xamarin.com/schemas/2014/forms"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            x:Class="Landmarks.Views.AppShell"
            xmlns:pages = "clr-namespace:Landmarks.Views;assembly=Landmarks"
            FlyoutBehavior="Disabled">
    <FlyoutItem Title="Animals"
                FlyoutDisplayOptions="AsMultipleItems">
        <ShellContent Title="Landmark">
            <pages:MainPage/>
        </ShellContent>
        <ShellContent Title="Settings">  
            <pages:SettingsPage/>
        </ShellContent>
        <ShellContent Title="Saved Landmarks">  
            <pages:SavedLandmarksPage/>
        </ShellContent>
    </FlyoutItem>
</Shell>
